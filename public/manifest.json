
{
  "name": "DescriçãoPro",
  "short_name": "DescriçãoPro",
  "description": "Crie descrições de produtos otimizadas para SEO rapidamente com IA",
  "version": "1.0.0",
  "manifest_version": 1,
  "permissions": [
    "admin",
    "products"
  ],
  "application": {
    "id": 17194,
    "options": {
      "embedded": true
    },
    "interfaces": {
      "admin": {
        "pages": [
          {
            "path": "/nexo-admin",
            "name": "DescriçãoPro",
            "styles": {
              "width": "100%",
              "height": "100vh"
            }
          }
        ]
      }
    }
  },
  "scopes": [
    "read_products",
    "write_products",
    "read_store_profile"
  ],
  "author": {
    "name": "WeetHub",
    "email": "comercial@weethub.com",
    "url": "https://descricaopro.com.br"
  },
  "homepage_url": "https://descricaopro.com.br",
  "icons": {
    "16": "icons/icon-16x16.png",
    "32": "icons/icon-32x32.png",
    "48": "icons/icon-48x48.png",
    "64": "icons/icon-64x64.png",
    "128": "icons/icon-128x128.png"
  }
}
